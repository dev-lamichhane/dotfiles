  "main with className": {
    "prefix": "mainc",
    "body": ["<main className='$1'>$2</main>"]
  },
  "article": {
    "prefix": "article",
    "body": ["<article>$1</article>"]
  },
  "article with className": {
    "prefix": "articlec",
    "body": ["<article className='$1'>$2</article>"]
  },
  "section": {
    "prefix": "section",
    "body": ["<section>$1</section>"]
  },
  "section with className": {
    "prefix": "sectionc",
    "body": ["<section className='$1'>$2</section>"]
  },
  "multi lined arrow function skeleton": {
    "prefix": "dos",
    "body": ["($1)=>{$2}"]
  },
  "one line arrow function return in parenthesis": {
    "prefix": "uno",
    "body": ["($1)=>($2)"]
  },
  "passing props with same name": {
    "prefix": "prop",
    "body": ["$1={$1}"]
  },
  "console.log": {
    "prefix": "cl",
    "body": ["console.log('$1')"]
  },
  "useState": {
    "prefix": "us",
    "body": ["const [$1, set$2$1] = useState($3)"]
  },
  "useEffect": {
    "prefix": "ue",
    "body": ["useEffect(()=>{", "$1", "},[$2])"]
  },
  "arrow function": {
    "prefix": "af",
    "body": ["const $1 = ($2)=>{$3}"]
  },
  "function": {
    "prefix": "fn",
    "body": ["function $1(){}"]
  },
  "create object": {
    "prefix": "co",
    "body": ["const $1 = {$2}"]
  },
  "create array": {
    "prefix": "car",
    "body": ["const $1 = [$2]"]
  },
  "for loop": {
    "prefix": "for",
    "body": ["for (let $1 = $2; $1 $3 ; $1$4 ) {$5}"]
  },
  "while loop": {
    "prefix": "while",
    "body": ["while($1){$2}"]
  },
  "react functional component": {
    "prefix": "rfc",
    "body": [
      "const $1 = ($2) => {$3}",
      "",
      "export default $1"
    ]
  }
}
