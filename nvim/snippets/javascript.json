  },
  "h3": {
    "prefix": "h3",
    "body": ["<h3>$1</h3>"]
  },
  "h3 with className": {
    "prefix": "h3c",
    "body": ["<h3 className='$1'>$2</h3>"]
  },
  "h4": {
    "prefix": "h4",
    "body": ["<h4>$1</h4>"]
  },
  "h4 with className": {
    "prefix": "h4c",
    "body": ["<h4 className='$1'>$2</h4>"]
  },
  "p": {
    "prefix": "p",
    "body": ["<p>$1</p>"]
  },
  "p with className": {
    "prefix": "pc",
    "body": ["<p className='$1'>$2</p>"]
  },
  "div": {
    "prefix": "div",
    "body": ["<div>$1</div>"]
