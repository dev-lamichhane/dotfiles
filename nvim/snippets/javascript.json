{
  "image": {
    "prefix": "img",
    "body": ["<img src={$1} alt='$2' />"]
  },
  "image with className": {
    "prefix": "imgc",
    "body": ["<img src={$1} alt='$2' className=$3 />"]
  },
  "unordered list": {
    "prefix": "ul",
    "body": ["<ul>$1</ul>"]
  },
  "unordered list with className": {
    "prefix": "ulc",
    "body": ["<ul className=$1>$2</ul>"]
  },
  "list item": {
    "prefix": "li",
    "body": ["<li>$1</li>"]
  },
  "list item with className": {
    "prefix": "lic",
    "body": ["<li className=$1>$2</li>"]
  },
  "component": {
    "prefix": "cmp",
    "body": ["< $1 />"]
  },
  "button": {
    "prefix": "btn",
    "body": ["<button>$1</button>"]
  },
  "button with className": {
    "prefix": "btnc",
    "body": ["<button className=$1>$2</button>"]
  },
  "input": {
    "prefix": "input",
    "body": ["<input $1 />"]
  },
  "ternary": {
    "prefix": "tern",
    "body": ["$1 ? $2 : $3"]
  },
  "react fragment": {
    "prefix": "frag",
    "body": ["return <>$1</>"]
  },
  "h1": {
    "prefix": "h1",
    "body": ["<h1>$1</h1>"]
  },
  "h1 with className": {
    "prefix": "h1c",
