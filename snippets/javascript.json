{
  "arrow function": {
    "prefix": "fn",
    "body": [
      "const $1 = ($2) =>{",
      "return $3",
      "}"
    ]
  },
  "function": {
    "prefix": "func",
    "body": [
      "function $1() {",
      "$2",
      "return $3",
      "}"
    ]
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for (let ${1:index} = 0; ${1:index} < $2; ${1:index}++) {",
      "\t${3}",
      "\t$0",
      "}"
    ],
    "discreption":"custom for"
  },
  "react fragment": {
    "prefix": "frag",
    "body": [
      "return <>$1</>"
    ]
  }
