{
  "console.log": {
    "prefix": "cl",
    "body": [
      "console.log('$1')"
    ]
  },
  "useState": {
    "prefix": "us",
    "body": [
      "const [$1, set${1/(.*)/${1:/capitalize}/}] = useState($2)"
    ]
  },
  "useEffect": {
    "prefix": "ue",
    "body": [
      "useEffect(()=>{",
      "$1",
      "},[$2])"
    ]
  },
  "arrow function": {
    "prefix": "af",
    "body": [
      "const $1 = ($2)=>{$3}"
    ]
  },
  "function": {
    "prefix": "fn",
    "body": [
      "function $1(){}"
    ]
  },
  "create object": {
    "prefix": "co",
    "body": [
      "const $1 = {$2}"
    ]
  },
  "create array": {
    "prefix": "car",
    "body": [
      "const $1 = [$2]"
    ]
  },
  "for loop": {
    "prefix": "for",
    "body": [
      "for (let $1) {$2}"
    ]
  },
  "react functional component": {
    "prefix": "rfc",
    "body": [
      "import {} from '.react",
      "",
      "const $1 = ($2) => {$3}",
      "",
      "export default $1"
    ]
  }
}
